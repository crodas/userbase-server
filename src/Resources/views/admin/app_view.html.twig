{% extends "admin/layout.html.twig" %}

{% block content %}
<h2>Apps: {{ viewapp.name }}</h2>

<div class="row">
    <div class="col-md-4">
        <img class="profile" src="{{ viewapp.getPictureUrl() }}" />
    </div>
    <div class="col-md-4">
        <ul>
            <li>Username: {{ viewapp.getDisplayName() }}</li>
            <li>E-mail: {{ viewapp.getBaseUrl() }}</li>
            <li>Created at: {{ viewapp.getCreatedAt()|mydate }}</li>
            <li>About: {{ viewapp.getAbout() }}</li>
        </ul>
    </div>
    <div class="col-md-4">
        <ul>
           
        </ul>
    </div>
</div>

{% endblock %}
