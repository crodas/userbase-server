{% extends "admin/layout.html.twig" %}

{% block content %}
<h2>Users: {{ usercount }}</h2>

<table class="table">
    <tr>
        <th>Username</th>
    </tr>
    {% for user in users %}
    <tr>
        <td><a href="/admin/users/{{ user.getUsername() }}">{{ user.getUsername() }}</a></td>
    </tr>
    {% endfor %}
</table>

{% endblock %}
